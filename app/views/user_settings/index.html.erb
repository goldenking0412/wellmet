<p id="notice"><%= notice %></p>

<h1>Listing User Settings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Radius</th>
      <th>Common interests count</th>
      <th>Gender</th>
      <th>Ages</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_settings.each do |user_setting| %>
      <tr>
        <td><%= user_setting.user_id %></td>
        <td><%= user_setting.radius %></td>
        <td><%= user_setting.common_interests_count %></td>
        <td><%= user_setting.gender %></td>
        <td><%= user_setting.ages %></td>
        <td><%= link_to 'Show', user_setting %></td>
        <td><%= link_to 'Edit', edit_user_setting_path(user_setting) %></td>
        <td><%= link_to 'Destroy', user_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User setting', new_user_setting_path %>
